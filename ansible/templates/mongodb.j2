sharding:
   clusterRole: shardsvr
replication:
   replSetName: replicaset_A
net:
   bindIp: localhost,{{ansible_default_ipv4.address}}
   port: 27018
storage:
   dbPath: /data/db
processManagement:
   fork: true
systemLog:
   destination: file
   path: "/var/log/mongodb/mongod.log"
   logAppend: true
